<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buildbody">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GetFIT"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">

        <activity android:name=".Forgot" />
        <activity android:name=".BMI" />
        <activity android:name=".Payment" />
        <activity android:name=".Kneeling" />
        <activity android:name=".LungeBow" />
        <activity android:name=".SideLying" />
        <activity android:name=".LyingTwist" />
        <activity android:name=".Bow" />
        <activity android:name=".Pelvic" />
        <activity android:name=".Trainer" />
        <activity android:name=".Bodyweight" />
        <activity android:name=".Warm" />
        <activity android:name=".DeskJob" />
        <activity android:name=".Posture" />
        <activity android:name=".About" />
        <activity android:name=".Profile" />

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true" />

        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignUp" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>